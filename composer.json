{
    "name": "dmkzwo/contao-list-example-bundle",
    "type": "contao-bundle",
    "description": "Contao backend list example",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Dmkzwo",
            "homepage": "https://www.dmkzwo.de"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.7",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Dmkzwo\\ContaoListExampleBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Dmkzwo\\ContaoListExampleBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmkzwo\\ContaoListExampleBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/dmkzwo/contao-list-example-bundle/issues",
        "source": "https://github.com/dmkzwo/contao-list-example-bundle"
    }
}
